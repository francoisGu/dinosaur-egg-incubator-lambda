{
  "name": "dinosaur-egg-incubator",
  "version": "1.0.0",
  "description": "Serverless for Dinosaur Egg Incubator",
  "main": "handler.js",
  "scripts": {
    "test-no-cleanup": "sls dynamodb start --migrate; mocha -r ts-node/register **/*.spec.ts",
    "test": "set +e; kill -9 $(lsof -t -i:8000); set -e; npm run test-no-cleanup; kill -9 $(lsof -t -i:8000)",
    "verify": "mocha -r ts-node/register **/*.spec.int.ts",
    "coverage-no-cleanup": "sls dynamodb install && sls dynamodb start --migrate && nyc --reporter=lcov --reporter=text mocha -r ts-node/register -r source-map-support/register --reporter mocha-multi-reporters --reporter-options configFile=../mocha-reports.json **/*.spec.ts **/*.spec.int.ts",
    "precoverage": "kill -9 $(lsof -t -i:8000); exit 0",
    "postcoverage": "kill -9 $(lsof -t -i:8000)",
    "coverage": "npm run coverage-no-cleanup",
    "sonar": "node ../sonarqube.js",
    "lint": "tslint --project ."
  },
  "dependencies": {
    "aws-sdk": "^2.453.0",
    "crypto-random-string": "^3.0.0",
    "lambda-log": "^2.2.0",
    "moment": "^2.24.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/aws-lambda": "^8.10.17",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "md-to-pdf": "^2.7.1",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.23.0",
    "mocha-multi-reporters": "^1.1.7",
    "mocha-sonarqube-reporter": "^1.0.1",
    "nyc": "^14.1.1",
    "serverless-dynamodb-local": "^0.2.37",
    "serverless-offline": "^4.10.0",
    "serverless-plugin-git-variables": "^3.3.2",
    "serverless-webpack": "^5.3.0",
    "source-map-support": "^0.5.12",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0"
  },
  "author": "Frank Gu",
  "license": "UNLICENSED"
}
